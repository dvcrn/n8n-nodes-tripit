    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "prettier": "^2.7.1",
    "typescript": "~4.6.4"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "n8n-workflow": "^1.48.0"
